{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --bun run vite",
    "build": "bun --bun run tsc && bun --bun run vite build",
    "lint": "bun --bun run eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "bun --bun run vite preview",
    "format": "bun --bun run prettier --write 'src/**/*.{ts,tsx,css}'",
    "type-check": "bun --bun run tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/vite": "^4.0.12",
    "axios": "^1.8.2",
    "framer-motion": "^12.4.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.2.0",
    "@types/bun": "latest"
  },
  "module": "index.ts"
}